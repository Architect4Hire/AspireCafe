        }
      },
      "kitchenCluster": {
        "Destinations": {
          "kitchen": {
            "Address": "http://localhost:5004/"
          }
        }
      },
      "productCluster": {
        "Destinations": {
          "product": {
            "Address": "http://localhost:52740/"
          }
        }
      },
      "ordersummaryCluster": {
        "Destinations": {
          "ordersummary": {
            "Address": "http://localhost:60472/"
          }
        }
      }
    }
  }
}